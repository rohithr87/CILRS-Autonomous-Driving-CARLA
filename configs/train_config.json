{
  "model": {
    "backbone": "resnet34",
    "pretrained": false,
    "num_commands": 4,
    "visual_feature_dim": 512,
    "speed_encoder_dims": [1, 128, 128],
    "combined_dim": 640,
    "branch_dims": [640, 256, 256, 3],
    "speed_predictor_dims": [512, 256, 256, 1],
    "dropout": 0.0,
    "total_parameters": 22400000
  },

  "input": {
    "image_width": 200,
    "image_height": 88,
    "image_channels": 3,
    "speed_normalization_factor": 40.0,
    "image_mean": [0.485, 0.456, 0.406],
    "image_std":  [0.229, 0.224, 0.225]
  },

  "training": {
    "epochs": 20,
    "batch_size": 128,
    "learning_rate": 0.0002,
    "weight_decay": 0.0001,
    "optimizer": "Adam",
    "loss_controls": "MSE",
    "loss_speed": "MSE",
    "speed_loss_weight": 0.05,
    "gradient_clip": null,
    "early_stopping_patience": null
  },

  "dataset": {
    "raw_image_width": 800,
    "raw_image_height": 600,
    "camera_fov_degrees": 100,
    "total_frames": 176256,
    "sessions": 5,
    "frames_per_session_approx": 35000,
    "train_split": 0.85,
    "val_split": 0.15,
    "simulator": "CARLA 0.9.10",
    "map": "Town01",
    "label_columns": ["steer", "throttle", "brake", "speed_kmh", "command"]
  },

  "hardware": {
    "gpu": "NVIDIA T4",
    "platform": "Kaggle",
    "mixed_precision": false,
    "num_workers": 4
  },

  "best_checkpoint": {
    "epoch": 20,
    "val_loss": 0.0538,
    "steer_correlation": 0.9861,
    "throttle_correlation": 0.9589,
    "brake_correlation": 0.9815,
    "speed_correlation": 0.9844,
    "filename": "checkpoint_best.pth"
  },

  "commands": {
    "0": "FOLLOW",
    "1": "LEFT",
    "2": "RIGHT",
    "3": "STRAIGHT"
  }
}
